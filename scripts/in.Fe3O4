units		real
atom_style	charge

boundary	m m m

read_data	data_init/Fe3O4_ovito.lmp
replicate	5 5 5

pair_style	reax/c data_init/lmp_control
pair_coeff	* * data_init/ffield.reax.Fe_O_C_H Fe O

neighbor	2 bin
neigh_modify	every 10 delay 0 check no

fix		verle all nve
fix		reax all qeq/reax 1 0.0 10.0 1.0e-6 reax/c
fix		thermostat all temp/berendsen 100.0 100.0 10.0

timestep	0.20
thermo		100

dump		ovito all atom 100 ovito_100.atom

run		20000

#write_restart Fe3O4.restart
